{
  "extends": ["config:base", "regexManagers:dockerfileVersions"],
  "regexManagers": [
    {
      "fileMatch": [
        "Dockerfile"
      ],
      "currentValueTemplate": "main",
      "depNameTemplate": "cacher",
      "packageNameTemplate": "https://github.com/packethost/cacher",
      "datasourceTemplate": "git-refs"
    }
  ]
}
